<?xml version="1.0" encoding="UTF-8"?>
<database>

    <global>
        <table name="currency">
            <field type="varchar(5) COLLATE utf8_unicode_ci NOT NULL">currency</field>
            <field type="varchar(255)">rate</field>
            <field type="int(1) DEFAULT 0">autoupdate</field>
            <field type="int(2) DEFAULT 2">precision</field>

            <field type="varchar(255) NOT NULL DEFAULT 'default'">type</field>
            <field type="TEXT NULL DEFAULT NULL">customData</field>

            <primary>currency</primary>
            <index>currency</index>
        </table>
    </global>

</database>
